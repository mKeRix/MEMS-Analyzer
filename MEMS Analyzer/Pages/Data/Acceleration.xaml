<UserControl x:Class="MEMS_Analyzer.Pages.Data.Acceleration"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="Tab" SelectedSource="/Content/Data/AccelData.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="DATEN" Source="/Content/Data/AccelData.xaml" />
                <mui:Link DisplayName="VISUELL" Source="/Pages/Data/AccelVisual.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
