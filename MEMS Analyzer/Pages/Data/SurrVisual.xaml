<UserControl x:Class="MEMS_Analyzer.Pages.Data.SurrVisual"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Content/Data/SurrVisualPressure.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Luftdruck" Source="/Content/Data/SurrVisualPressure.xaml" />
                <mui:Link DisplayName="Temperatur" Source="/Content/Data/SurrVisualTemperature.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
