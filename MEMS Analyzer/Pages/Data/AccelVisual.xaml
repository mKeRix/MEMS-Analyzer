<UserControl x:Class="MEMS_Analyzer.Pages.Data.AccelVisual"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Content/Data/AccelVisualG.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Achsen in g" Source="/Content/Data/AccelVisualG.xaml" />
                <mui:Link DisplayName="Achsen in m/s^2" Source="/Content/Data/AccelVisualMeter.xaml" />
                <mui:Link DisplayName="Beträge in g" Source="/Content/Data/AccelVisualSumG.xaml" />
                <mui:Link DisplayName="Beträge in m/s^2" Source="/Content/Data/AccelVisualSumMeter.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
